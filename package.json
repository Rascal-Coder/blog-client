  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "@rascal/rules-config": "workspace:*"
  },
  "devDependencies": {
    "@unocss/preset-uno": "0.59.4",
    "@unocss/core": "0.59.4"
  }
}
