{
  "name": "blog-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:main": "cd apps/main && pnpm dev",
    "dev:editor": "cd apps/editor && pnpm dev",
    "dev": "npm-run-all dev:*",
